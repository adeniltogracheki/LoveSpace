{
  "expo": {
    "name": "LoveSpace",
    "slug": "lovespace",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "lovespace",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FFF0F5"
    },
        "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lovespace.app"
    },
    "android": {
      "package": "com.lovespace.app"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFF0F5",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}